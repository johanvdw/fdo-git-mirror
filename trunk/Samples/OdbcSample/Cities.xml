<?xml version="1.0" encoding="UTF-8"?>
<fdo:DataStore 
    xmlns:fds="http://fdo.osgeo.org/schemas/fds"
    xmlns:fdo="http://fdo.osgeo.org/schemas"
    xmlns:gml="http://www.opengis.net/gml"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:xlink="http://www.w3.org/1999/xlink"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
>
	<gml:DerivedCRS gml:id="WGS84">
		<gml:remarks>LongitudeLatitudeWGS84</gml:remarks>
		<gml:srsName>WGS84</gml:srsName>
		<gml:validArea>
			<gml:boundingBox>
				<gml:pos>-10.000000 90.000000</gml:pos>
				<gml:pos>150.000000 70.000000</gml:pos>
			</gml:boundingBox>
		</gml:validArea>
		<gml:baseCRS>
			<fdo:WKTCRS gml:id="LongitudeLatitudeWGS84">
				<gml:srsName>Longitude / Latitude (WGS 84)</gml:srsName>
				<fdo:WKT>GEOGCS [ "Longitude / Latitude (WGS 84)", DATUM ["WGS 84", SPHEROID ["WGS 84", 6378137, 298.257223563]], PRIMEM [ "Greenwich", 0.000000 ], UNIT "Decimal Degree", 0.01745329251994330]]</fdo:WKT>
			</fdo:WKTCRS>
		</gml:baseCRS>
		<gml:definedByConversion xlink:href="http://fdo.osgeo.org/coord_conversions#identity"/>
		<gml:derivedCRSType codeSpace="http://fdo.osgeo.org/crs_types">geographic</gml:derivedCRSType>
		<gml:usesCS xlink:href="http://fdo.osgeo.org/cs#default_cartesian"/>
	</gml:DerivedCRS>

	<xs:schema 
        xmlns:xs="http://www.w3.org/2001/XMLSchema" 
        targetNamespace="http://www.oceanographic.org/World" 
        xmlns:fdo="http://fdo.osgeo.org/schemas" 
        xmlns:World="http://www.oceanographic.org/World" 
        elementFormDefault="qualified" 
        attributeFormDefault="qualified"
	>
		<xs:element name="Cities" type="World:CitiesType" abstract="false" substitutionGroup="gml:_Feature">
			<xs:key name="CitiesKey"> 
  				<xs:selector xpath=".//Cities"/>
				<xs:field xpath="Id"/>
			</xs:key>
 		</xs:element>
		<xs:complexType 
		    name="CitiesType" 
		    abstract="false" 
		    fdo:geometryName="Geometry" 
		    fdo:geometricTypes="Point" 
		    fdo:hasMeasure="false" 
		    fdo:hasElevation="false"
        >
			<xs:complexContent>
				<xs:extension base="gml:AbstractFeatureType">
					<xs:sequence>
						<xs:element name="Id" type="fdo:int64" />
						<xs:element name="Name" type="xs:string" />
						<xs:element name="Country" type="xs:string" />
						<xs:element name="Population" type="xs:double" />
						<xs:element name="Capital" type="xs:string" />
						<xs:element name="URL" type="xs:string" />
		        	</xs:sequence>
      			</xs:extension>
   			</xs:complexContent>
		</xs:complexType>
	</xs:schema>

	<SchemaMapping 
	    name="World" 
	    provider="OSGeo.ODBC.3.2" 
	    physicalSchemaName="" 
	    xmlns="http://fdoodbc.osgeo.org/schemas"
	>
		<complexType name="CitiesType">
			<Table name="Cities" xColumnName="LONGITUDE" yColumnName="LATITUDE" zColumnName=""/>
			<element name="Id">
				<Column name="CITY_ID"/>
			</element>
			<element name="Name">
				<Column name="NAME"/>
			</element>
			<element name="Country">
				<Column name="COUNTRY"/>
			</element>
			<element name="Population">
				<Column name="POPULATION"/>
			</element>
			<element name="Capital">
				<Column name="CAPITAL"/>
			</element>
			<element name="URL">
				<Column name="URL"/>
			</element>
		</complexType>
	</SchemaMapping>
</fdo:DataStore>
